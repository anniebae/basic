<div class="signup-form">
<h1 class="new-user-form">SIGN UP</h1>
<%= form_for(@user) do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
  <br>
	<%= f.label :password %>
	<%= f.text_field :password %>
  <br>
	<%= f.label :instagram_account %>
	<%= f.text_field :instagram_account %>
  <br>
	<%= f.submit :submit %>


<% end %>

</div>